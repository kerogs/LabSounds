{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "labsounds",
  "version": "1.0.0",
  "identifier": "com.labsounds.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "LabSounds",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/apple-touch-icon.png",
      "icons/icon-192.png",
      "icons/icon-512.png",
      "icons/favicon.ico"
    ]
  }
}
